{
	"name": "idr-web",
	"private": true,
	"version": "0.0.0",
	"scripts": {
		"dev": "vite",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"format": "prettier . --cache",
		"format:fix": "pnpm format --write",
		"format:check": "pnpm format --check",
		"lint": "eslint --ext .ts,.tsx --cache --max-warnings 0 .",
		"lint:fix": "pnpm lint --fix",
		"lint:check": "pnpm lint",
		"type:check": "tsc --noEmit",
		"lint:staged": "lint-staged",
		"storybook:dev": "storybook dev -p 6006",
		"storybook:build": "storybook build -o docs-build",
		"seed:mock": "node ./scripts/seed/index.mjs"
	},
	"dependencies": {
		"@hookform/resolvers": "^3.9.0",
		"@radix-ui/react-alert-dialog": "^1.1.2",
		"@radix-ui/react-avatar": "^1.1.1",
		"@radix-ui/react-dialog": "^1.1.2",
		"@radix-ui/react-dropdown-menu": "^2.1.2",
		"@radix-ui/react-label": "^2.1.0",
		"@radix-ui/react-popover": "^1.1.2",
		"@radix-ui/react-slot": "^1.1.0",
		"@radix-ui/react-tabs": "^1.1.1",
		"@radix-ui/react-tooltip": "^1.1.3",
		"@tanstack/react-query": "^5.59.9",
		"@tanstack/react-table": "^8.20.5",
		"axios": "^1.7.7",
		"class-variance-authority": "^0.7.0",
		"clsx": "^2.1.1",
		"cmdk": "^1.0.0",
		"lucide-react": "^0.320.0",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-hook-form": "^7.53.0",
		"react-hot-toast": "^2.4.1",
		"react-router-dom": "^6.26.2",
		"tailwind-merge": "^2.5.3",
		"validator": "^13.12.0",
		"zod": "^3.23.8"
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.8.1",
		"@faker-js/faker": "^8.4.1",
		"@storybook/addon-actions": "^7.6.20",
		"@storybook/addon-essentials": "^7.6.20",
		"@storybook/addon-interactions": "^7.6.20",
		"@storybook/addon-links": "^7.6.20",
		"@storybook/addon-themes": "^7.6.20",
		"@storybook/preview-web": "^7.6.17",
		"@storybook/react": "^7.6.20",
		"@storybook/react-vite": "^7.6.20",
		"@tanstack/eslint-plugin-query": "^5.59.7",
		"@tanstack/react-query-devtools": "^5.59.9",
		"@types/react": "^18.3.11",
		"@types/react-dom": "^18.3.0",
		"@types/validator": "^13.12.2",
		"@typescript-eslint/eslint-plugin": "^6.21.0",
		"@typescript-eslint/parser": "^6.21.0",
		"@vitejs/plugin-react": "^4.3.2",
		"autoprefixer": "^10.4.20",
		"commitlint": "^17.8.1",
		"eslint": "^8.57.1",
		"eslint-config-airbnb": "^19.0.4",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-import": "^2.31.0",
		"eslint-plugin-jsx-a11y": "^6.10.0",
		"eslint-plugin-prettier": "^5.2.1",
		"eslint-plugin-react-hooks": "^4.6.2",
		"eslint-plugin-storybook": "^0.6.15",
		"husky": "^8.0.3",
		"lint-staged": "^13.3.0",
		"msw": "^2.4.9",
		"postcss": "^8.4.47",
		"prettier": "^3.3.3",
		"storybook": "^7.6.20",
		"tailwindcss": "^3.4.13",
		"tailwindcss-animate": "^1.0.7",
		"typescript": "^5.3.3",
		"vite": "^4.5.5"
	},
	"lint-staged": {
		"*.{ts,tsx}": [
			"pnpm lint:check"
		],
		"*.{json,js,jsx,ts,tsx,htm,html,md}": "pnpm format:check"
	},
	"packageManager": "pnpm@7.26.3",
	"engines": {
		"node": ">=18.0.0",
		"npm": ">=6.14.13"
	},
	"msw": {
		"workerDirectory": [
			"public"
		]
	}
}
